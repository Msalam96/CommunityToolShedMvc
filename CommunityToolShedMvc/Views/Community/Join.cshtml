@model IList<CommunityToolShedMvc.Models.Community>

@{
    ViewBag.Title = "Join";
}

<h2>Join</h2>

<div style="width: 35%">
    <table class="table table-striped table-bordered table-hover table-sm">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Community Name</th>
                <th>&nbsp;</th>
            </tr>
        </thead>
        @foreach (var community in Model)
        {
            <tr>
                <td scope="col">@community.CommunityName</td>
            </tr>
        }
    </table>
</div>